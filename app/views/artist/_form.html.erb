<div class="form-group">
	<label>Name Artist</label>
	<%= f.text_field :name, value: @artist.name, placeholder: 'Name', class: 'form-control', required: true %>
</div>

<div class="form-group">
	<label>Description</label>
	<%= f.text_field :description, value: @artist.description, placeholder: 'Description', class: 'form-control', required: true %>
</div>

<%= f.submit 'Create', class: 'btn btn-primary' %>